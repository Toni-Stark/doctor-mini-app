/* pages/tabView/index.wxss */
.nav-tabs {
    /* background-color: darkcyan; */
    background-color: #ffffff;
    width: 100%;
    display: flex;
    position: fixed;
    bottom: 0;
    align-items: center;
    z-index: 999;
    padding-top: 15rpx;
    border-top: 1rpx solid #f2f2f2;
    height: 9.5vh;
  }
  .tab-list {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-bottom: 10px;
  }

  .tab-list view {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #afafaf;
  }
  .active view {
    color: #000000;
  }
  .tab-content {
    flex: 1;
  }
   
  .default {
    flex: 1;
    color: #000;
    font-weight: bold;
    font-size: 28rpx;
  }
   
  .active {
    color: #fc5558;
    flex: 1;
    font-weight: bold;
    font-size: 28rpx;
  }
  .beginDo {
    border:black 1px solid;
    background-color: burlywood;
    border-radius: 100%;
    height: 60px;
    width: 60px;
    margin-bottom: 20px;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
  }